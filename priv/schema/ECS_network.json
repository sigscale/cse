{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "http://sigscale.org/schema/ECS_network.json",
	"title": "Elastic Common Schema (ECS) Network",
	"description": "The network is defined as the communication path over which a host or network event happens.",
	"type": "object",
	"properties": {
		"application": {
			"description": "Application level protocol name.",
			"type": "string",
			"examples": [
				"ro",
				"nrf"
			]
		},
		"protocol": {
			"description": "Application protocol name.",
			"type": "string",
			"examples": [
				"diameter",
				"http"
			]
		}
	}
}
